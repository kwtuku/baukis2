<% @title = "許可IPアドレス⼀覧" %>
<h1><%= @title %></h1>

<div class="table-wrapper">
  <table class="listing">
    <tr>
      <th>IPアドレス</th>
      <th>作成⽇時</th>
    </tr>
    <% @allowed_sources.each do |s| %>
      <% p = AllowedSourcePresenter.new(s, self) %>
      <tr>
        <td class="ip"><%= p.ip_address %></td>
        <td class="date"><%= p.created_at %></td>
      </tr>
    <% end %>
  </table>
</div>
